<Page x:Class="Bibon.Pages.Store.Window3"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Bibon.Pages.Store"
        mc:Ignorable="d"
        Title="Window3">
    <Border CornerRadius="20,20,20,20" Background="{DynamicResource SecundaryBackgroundColor1}">


        <Grid>
            <StackPanel VerticalAlignment="Bottom" Orientation="Horizontal" Margin="10,20,10,0" HorizontalAlignment="Center">




                <Border 
Style="{DynamicResource CardBorderStyle1}" Padding="0,0,0,0" Width="240" >

                    <Grid VerticalAlignment="Center">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>






                        <Grid Grid.Row="0">
                            <Image Source="/Assets/Store/58.png" Stretch="Uniform" Width="55" Height="45" VerticalAlignment="Top" HorizontalAlignment="Center" Panel.ZIndex="1" Margin="0,-20,0,0">

                            </Image>
                            <Grid Height="50">



                                <Button x:Name="OpenWebsiteButton12" Click="OpenWebsite12_Click" Style="{DynamicResource PrimaryButtonStyle1}" Content="Euro Truck Simulator 2" HorizontalAlignment="Center" VerticalAlignment="Center">

                                </Button>
                            </Grid>
                        </Grid>

                        <Grid Grid.Row="1" Margin="0,30,0,0">
                            <Image Source="/Assets/Store/12.png" Stretch="Uniform" Width="35" Height="35" VerticalAlignment="Top" HorizontalAlignment="Center" Panel.ZIndex="1" Margin="0,-20,0,0"/>
                            <Grid Height="50">
                                <Button x:Name="OpenWebsiteButton11" Click="OpenWebsite11_Click" Style="{DynamicResource PrimaryButtonStyle1}" Content="OBS" HorizontalAlignment="Center" VerticalAlignment="Center">

                                </Button>
                            </Grid>
                        </Grid>

                        <Grid Grid.Row="2" Margin="0,30,0,0">
                            <Image Source="/Assets/Store/16.png" Stretch="Uniform" Width="35" Height="35" VerticalAlignment="Top" HorizontalAlignment="Center" Panel.ZIndex="1" Margin="0,-20,0,0"/>
                            <Grid Height="50">
                                <Button x:Name="OpenWebsiteButton10" Click="OpenWebsite10_Click" Style="{DynamicResource PrimaryButtonStyle1}" Content="µTorrent" HorizontalAlignment="Center" VerticalAlignment="Center">

                                </Button>
                            </Grid>
                        </Grid>

                        <Grid Grid.Row="3" Margin="0,30,0,0">
                            <Image Source="/Assets/Store/8.png" Stretch="Uniform" Width="35" Height="35" VerticalAlignment="Top" HorizontalAlignment="Center" Panel.ZIndex="1" Margin="0,-20,0,0"/>
                            <Grid Height="50">
                                <Button x:Name="OpenWebsiteButton9" Click="OpenWebsite9_Click" Style="{DynamicResource PrimaryButtonStyle1}" Content="Epic Games" HorizontalAlignment="Center" Cursor="Hand" VerticalAlignment="Center"/>
                            </Grid>
                        </Grid>

                        <Grid Grid.Row="4" Margin="0,30,0,0">
                            <Image Source="/Assets/Store/28.png" Stretch="Uniform" Width="35" Height="35" VerticalAlignment="Top" HorizontalAlignment="Center" Panel.ZIndex="1" Margin="0,-20,0,0"/>
                            <Grid Height="50">
                                <Button x:Name="OpenWebsiteButton8" Click="OpenWebsite8_Click" Style="{DynamicResource PrimaryButtonStyle1}" Content="ArtMoney" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Grid>
                        </Grid>

                        <Grid Grid.Row="5" Margin="0,30,0,0">
                            <Image Source="/Assets/Store/63.png" Stretch="Uniform" Width="35" Height="35" VerticalAlignment="Top" HorizontalAlignment="Center" Panel.ZIndex="1" Margin="0,-20,0,0"/>
                            <Grid Height="50">
                                <Button x:Name="OpenWebsiteButton7" Click="OpenWebsite7_Click" Style="{DynamicResource PrimaryButtonStyle1}" Content="Ruby" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Grid>
                        </Grid>

                    </Grid>
                </Border>




                <Border 
Style="{DynamicResource CardBorderStyle1}"  Width="240" Padding="0,0,0,0">

                    <Grid VerticalAlignment="Center">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>


                        <Grid Grid.Row="0">
                            <Image Source="/Assets/Store/62.png" Stretch="Uniform" Width="35" Height="35" VerticalAlignment="Top" HorizontalAlignment="Center" Panel.ZIndex="1" Margin="0,-20,0,0"/>
                            <Grid Height="50">
                                <Button x:Name="OpenWebsiteButton13" Click="OpenWebsite13_Click" Style="{DynamicResource PrimaryButtonStyle1}" Content="Python" HorizontalAlignment="Center" VerticalAlignment="Center"/>

                            </Grid>
                        </Grid>
                        <Grid Grid.Row="1" Margin="0,30,0,0">
                            <Image Source="/Assets/Store/27.png" Stretch="Uniform" Width="35" Height="35" VerticalAlignment="Top" HorizontalAlignment="Center" Panel.ZIndex="1" Margin="0,-20,0,0"/>
                            <Grid Height="50">
                                <Button x:Name="OpenWebsiteButton14" Click="OpenWebsite14_Click" Style="{DynamicResource PrimaryButtonStyle1}" Content="Traffic Monitor" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Grid>
                        </Grid>
                        <Grid Grid.Row="2" Margin="0,30,0,0">
                            <Image Source="/Assets/Store/71.png" Stretch="Uniform" Width="35" Height="35" VerticalAlignment="Top" HorizontalAlignment="Center" Panel.ZIndex="1" Margin="0,-20,0,0"/>
                            <Grid Height="50">
                                <Button x:Name="OpenWebsiteButton15" Click="OpenWebsite15_Click" Style="{DynamicResource PrimaryButtonStyle1}" Content="Unlocker" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Grid>
                        </Grid>
                        <Grid Grid.Row="3" Margin="0,30,0,0">
                            <Image Source="/Assets/Store/68.png" Stretch="Uniform" Width="35" Height="35" VerticalAlignment="Top" HorizontalAlignment="Center" Panel.ZIndex="1" Margin="0,-20,0,0"/>
                            <Grid Height="50">
                                <Button x:Name="OpenWebsiteButton16" Click="OpenWebsite16_Click" Style="{DynamicResource PrimaryButtonStyle1}" Content="SmoothScroll" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Grid>
                        </Grid>
                        <Grid Grid.Row="4" Margin="0,30,0,0">
                            <Image Source="/Assets/Store/69.png" Stretch="Uniform" Width="35" Height="35" VerticalAlignment="Top" HorizontalAlignment="Center" Panel.ZIndex="1" Margin="0,-20,0,0"/>
                            <Grid Height="50">
                                <Button x:Name="OpenWebsiteButton17" Click="OpenWebsite17_Click" FontSize="16" Style="{DynamicResource PrimaryButtonStyle1}" Content="Flowframes" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Grid>
                        </Grid>
                        <Grid Grid.Row="5" Margin="0,30,0,0">
                            <Image Source="/Assets/Store/70.png" Stretch="Uniform" Width="45" Height="45" VerticalAlignment="Top" HorizontalAlignment="Center" Panel.ZIndex="1" Margin="0,-20,0,0"/>
                            <Grid Height="50">
                                <Button x:Name="OpenWebsiteButton18" Click="OpenWebsite18_Click" Style="{DynamicResource PrimaryButtonStyle1}" Content="AI Image Generator" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Grid>
                        </Grid>
                    </Grid>
                </Border>



                <Border 
Style="{DynamicResource CardBorderStyle1}" Padding="0,0,0,0" Width="240">

                    <Grid VerticalAlignment="Center">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>

                        <Grid Grid.Row="0">
                            <Image Source="/Assets/Store/72.png" Stretch="Uniform" Width="35" Height="35" VerticalAlignment="Top" HorizontalAlignment="Center" Panel.ZIndex="1" Margin="0,-20,0,0"/>
                            <Grid Height="50">
                                <Button x:Name="OpenWebsiteButton24" Click="OpenWebsite24_Click" Style="{DynamicResource PrimaryButtonStyle1}" Content="WinDynamicDesktop" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Grid>
                        </Grid>
                        <Grid Grid.Row="1" Margin="0,30,0,0">
                            <Image Source="/Assets/Store/73.png" Stretch="Uniform" Width="35" Height="35" VerticalAlignment="Top" HorizontalAlignment="Center" Panel.ZIndex="1" Margin="0,-25,0,0"/>
                            <Grid Height="50">
                                <Button x:Name="OpenWebsiteButton23" Click="OpenWebsite23_Click" Style="{DynamicResource PrimaryButtonStyle1}" Content="Adobe Animate" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Grid>
                        </Grid>
                        <Grid Grid.Row="2" Margin="0,30,0,0">
                            <Image Source="/Assets/Store/74.png" Stretch="Uniform" Width="35" Height="35" VerticalAlignment="Top" HorizontalAlignment="Center" Panel.ZIndex="1" Margin="0,-20,0,0"/>
                            <Grid Height="50">
                                <Button x:Name="OpenWebsiteButton22" Click="OpenWebsite22_Click" Style="{DynamicResource PrimaryButtonStyle1}" Content="Davinci Resolve Studio" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Grid>
                        </Grid>
                        <Grid Grid.Row="3" Margin="0,30,0,0">
                            <Image Source="/Assets/Store/75.png" Stretch="Uniform" Width="45" Height="45" VerticalAlignment="Top" HorizontalAlignment="Center" Panel.ZIndex="1" Margin="0,-30,0,0"/>
                            <Grid Height="50">
                                <Button x:Name="OpenWebsiteButton21" Click="OpenWebsite21_Click" Style="{DynamicResource PrimaryButtonStyle1}" Content="Topaz Video AI " HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Grid>
                        </Grid>
                        <Grid Grid.Row="4" Margin="0,30,0,0">
                            <Image Source="/Assets/Store/76.png" Stretch="Uniform" Width="35" Height="35" VerticalAlignment="Top" HorizontalAlignment="Center" Panel.ZIndex="1" Margin="0,-20,0,0"/>
                            <Grid Height="50">
                                <Button x:Name="OpenWebsiteButton20" Click="OpenWebsite20_Click" Style="{DynamicResource PrimaryButtonStyle1}" Content="VEGAS PRO" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Grid>
                        </Grid>
                        <Grid Grid.Row="5" Margin="0,30,0,0">
                            <Image Source="/Assets/Store/1.png" Stretch="Uniform" Width="35" Height="35" VerticalAlignment="Top" HorizontalAlignment="Center" Panel.ZIndex="1" Margin="0,-20,0,0"/>
                            <Grid Height="50">
                                <Button x:Name="OpenWebsiteButton19" Click="OpenWebsite19_Click" Style="{DynamicResource PrimaryButtonStyle1}" Content="Android Studio" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Grid>
                        </Grid>
                    </Grid>
                </Border>



            </StackPanel>


        </Grid>




    </Border>
</Page>
