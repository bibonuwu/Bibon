<Page x:Class="WPFUIKitProfessional.Pages.Collections"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WPFUIKitProfessional.Pages"
      mc:Ignorable="d" 
            xmlns:wv2="clr-namespace:Microsoft.Web.WebView2.Wpf;assembly=Microsoft.Web.WebView2.Wpf"

      d:DesignHeight="450" d:DesignWidth="800"
     FontFamily="Poppins">

    <Border CornerRadius="8" Background="{DynamicResource SecundaryBackgroundColor}">
        <Grid>
            <ProgressBar Name="progressBar" Height="10" Minimum="0" Maximum="100" Width="300" Visibility="Hidden" Foreground="#32D74B" />
            <wv2:WebView2 Name="webView" Source="https://sites.google.com/view/abekenaik/app/"/>


        </Grid>
    </Border>
</Page>
